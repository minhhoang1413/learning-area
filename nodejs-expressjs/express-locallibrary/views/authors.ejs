<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head',{title }) %>


    <body>
        <%- include('./partials/navbar') %>
        <h1>
            <%= title %>
        </h1>
        
        <ul>
            <% authors.forEach(author => { %>
                <li> 
                    <a href="<%= author.url %>">
                        <%= `${author.fullname} ${author.date}` %> 
                    </a>
                </li>
            <% }) %>

        </ul>
    </body>

</html>