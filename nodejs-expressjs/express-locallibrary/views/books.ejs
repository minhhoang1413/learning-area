<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head',{title }) %>


    <body>
        <%- include('./partials/navbar') %>
        <h1>
            <%= title %>
        </h1>
        
        <ul>
            <% books.forEach(book => { %>
                <li> 
                    <a href="<%= book.url %>">
                        <%= `${book.title} (${book.author.fullname})` %> 
                    </a>
                </li>
            <% }) %>

        </ul>
    </body>

</html>